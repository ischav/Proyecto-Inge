@model ProyectoInge1.Models.CriterioAceptacionHistorial
@using ProyectoInge1.Models

<div class="criterios">

    @Html.HiddenFor(model => model.Borrar, new { @class = "mark-for-delete" })

    <div class="row">
        @Html.LabelFor(model => model.Descripcion, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-5">
            @Html.EditorFor(model => model.Descripcion, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        <div class="col-md-1">
            @Html.RemoveLink("Eliminar", "div.criterios", "input.mark-for-delete")
        </div>
    </div>

</div>


