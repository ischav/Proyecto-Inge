@model ProyectoInge1.Models.ModeloIntermedio
@{
    ViewBag.Title = "Usuarios ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Clientes</h2>
<p>
    @Html.ActionLink("Ingresar usuario", "Create")
</p>

<table class="table">
    <tr>
        <th>
            Cedula
        </th>
        <th>
            Nombre
        </th>
        <th>
            Primer apellido
        </th>
        <th>
            Segundo apellido
        </th>
        <th>
            Fecha de Nacimiento
        </th>
        <th>
            Teléfono 1
        </th>
        <th>
            Teléfono 2
        </th>

    </tr>

    @for (int i = 0; i < Model.listaPersonas.Count; i++)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => Model.listaPersonas.ElementAt(i).Cedula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.listaPersonas.ElementAt(i).Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.listaPersonas.ElementAt(i).Apellido1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.listaPersonas.ElementAt(i).Apellido2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.listaPersonas.ElementAt(i).FechaNac)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.listaPersonas.ElementAt(i).Telefono1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.listaPersonas.ElementAt(i).Telefono2)
            </td>
            <td></td>

            <td>
                @Html.ActionLink("Detalles", "Details", new { id = Model.listaPersonas.ElementAt(i).Cedula })
            </td>
        </tr>
    }
</table>
